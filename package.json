{
  "name": "nfl-fantasy-pickem",
  "version": "0.0.2",
  "description": "Fantasy NFL football and pick'em solution using Sportradar API",
  "main": "server/index.js",
  "scripts": {
    "install-all": "npm install && cd client && npm install",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "./start.sh",
    "check": "./check.sh",
    "stop": "./stop.sh"
  },
  "keywords": ["nfl", "fantasy", "football", "pickem"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "sqlite3": "^5.1.6",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  }
}

